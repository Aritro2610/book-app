<h2>Recently Added Books</h2>

<div>
    <% books.forEach(book=>{%>
        <label><%= book.title %></label>
        <img height="150" width="100" src="<%= book.coverImagePath %>">
    <%})%>
</div>
<br>
<h2>Recently Added Authors</h2>

<div class="authors">
    <% authors.forEach(author=>{%>
        <div class="author_name"><%= author.name %></div>
    <%})%>
</div>

